  "nodes": {
    "S1": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Hey! Somepony here?.. Help me!",
      "config": "armored_soldier_phone",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S1(1)"
        }
      ]
    },
    "S2": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Lieutenant, help me! Those assholes gave me a defective nuclear battery again!",
      "config": "armored_soldier_phone",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "next": "S3"
    },
    "S3": {
      "kind": "dialogue",
      "root_node": false,
      "body": "The armor just suddenly shut down, and now I can't move...",
      "config": "armored_soldier_phone",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S3(1)"
        },
        {
          "body": "answer",
          "next": "S3(2)"
        }
      ]
    },
    "S4": {
      "kind": "dialogue",
      "root_node": false,
      "body": "It's been hours. I thought no one would help me...",
      "config": "armored_soldier_phone",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S3(2)"
        }
      ]
    },
    "S5": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Let me report it myself, Lieutenant. Just help me with this armor, please...",
      "config": "armored_soldier_phone",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S5(1)"
        }
      ]
    },
    "S6": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Yes, they should be in my closet in the 104th room. Cabinet on the left.",
      "config": "armored_soldier_phone",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S6(1)"
        }
      ]
    },
    "S1(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "It's lieutenant Strikely, soldier. Why are you standing here?",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S2"
    },
    "S3(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "How long have you been standing here like this?",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S4"
    },
    "S3(2)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Who did this? I'll report it to the Colonel.",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S5"
    },
    "S5(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "As you want. Do you have any spare batteries?",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S6"
    },
    "S6(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Oh, yes... I have an extra battery here. Let me replace it... [replace the battery]",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S6(1.1)"
    },
    "S7": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Oh, Celestia, finally!.. Wait, where did you get this battery? Did you open my closet?",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S7(1)"
        }
      ]
    },
    "S8": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Really, how could I think that of you? You're my savior... Thank you very much!",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "S8(1)"
        }
      ]
    },
    "S9": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Yes, ma'am! Good-bye, lieutenant",
      "speaker": "Armored soldier",
      "opposite": "Strikely",
      "next": "S9(1)"
    },
    "S7(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "No, of course not, soldier, how could you think that!",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S8"
    },
    "S8(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "You're welcome. Next time, check the batteries before you put them in.",
      "speaker": "Strikely",
      "opposite": "Armored soldier",
      "next": "S9"
