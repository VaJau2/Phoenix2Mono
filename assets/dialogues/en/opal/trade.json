      "next": "T6"
    },
    "T7": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Maybe you want to buy something?",
      "speaker": "Opal",
      "opposite": "Strikely",
      "options": [
        {
          "body": "answer",
          "next": "T7(1)"
        },
        {
          "body": "answer",
          "next": "T7(2)"
        }
      ]
    },
    "T7(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Yes. Show me what you have for sale.",
      "speaker": "Strikely",
      "opposite": "Opal",
      "next": "T7(1.1)"
    },
    "T7(2)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Sometime later. I have to go",
      "speaker": "Strikely",
      "opposite": "Opal",
      "next": "T8"
    },
    "T7(1.1)": {
      "kind": "narration",
