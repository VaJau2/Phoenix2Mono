    "B3(1)": {
      "kind": "dialogue",
      "root_node": false,
      "body": "But you haven't forgotten your beloved sister, have you?",
      "speaker": "Strikely",
      "opposite": "Ghost",
      "next": "B4"
    },
    "B4": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Sorry, I don't remember.",
      "speaker": "Ghost",
      "opposite": "Strikely",
