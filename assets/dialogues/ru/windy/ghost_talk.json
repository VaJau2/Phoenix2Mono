      "opposite": "Призрак",
      "next": "B7"
    },
    "B7": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Я тоже, сестренка. Я навсегда останусь в твоей памяти.",
      "speaker": "Призрак",
      "opposite": "Страйкли",
