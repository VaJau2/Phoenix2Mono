        },
        "E7(1)": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Я хочу их уничтожить. ",
            "speaker": "Страйкли",
            "opposite": "Таинственный Незнакомец",
            "next": "E8"
        },
        "E8": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Подожди… серьёзно? Ты здесь только за тем, чтобы сжечь эти жалкие бумажки?",
            "speaker": "Таинственный Незнакомец",
            "opposite": "Страйкли",
            "options": [
                {
                    "body": "answer",
                    "next": "E8(1)"
