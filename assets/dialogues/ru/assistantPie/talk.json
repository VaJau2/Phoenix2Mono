        "P1(1)": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Конечно, нас в армии этому хорошо учат!",
            "speaker": "Страйкли",
            "opposite": "Помощница Пай",
            "next": "P2"
        },
        "P1(2)": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Зачем прятаться, если у тебя есть оружие, и можно всех перестрелять?",
