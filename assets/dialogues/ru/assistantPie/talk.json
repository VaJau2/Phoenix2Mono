            "opposite": "Помощница Пай",
            "next": "P4"
        },
        "P3(2)": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Верю на слово",
            "speaker": "Страйкли",
            "opposite": "Помощница Пай",
            "next": "P4"
        },
        "P4": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Во-первых, врагов может быть много, а ты у мамы одна...",
            "speaker": "Помощница Пай",
            "timer": "0.01",
            "opposite": "Страйкли",
            "options": [
                {
                    "body": "answer",
                    "next": "P4(1)"
                },
                {
                    "body": "answer",
                    "next": "P4(2)"
                }
            ]
        },
        "P4(1)": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Дальше?",
