            "next": "P10"
        },
        "P10": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Не все так просто!",
            "speaker": "Помощница Пай",
            "timer": "0.01",
            "opposite": "Страйкли",
            "next": "P11"
        },
        "P11": {
            "kind": "dialogue",
            "root_node": false,
            "body": "Ты должна доказать, что умеешь красться, и достать его незаметно!",
            "speaker": "Помощница Пай",
            "timer": "0.01",
            "opposite": "Страйкли",
            "next": "P12"
