    },
    "R7": {
      "kind": "dialogue",
      "root_node": false,
      "body": "Опусти оружие и дай мне сбежать.",
      "speaker": "Рэд Панч",
      "opposite": "Страйкли",
      "options": [
        {
          "body": "answer",
          "next": "R7(2)"
